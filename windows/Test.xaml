        xmlns:local="clr-namespace:GUIForDiskpart.windows"
        xmlns:userControls="clr-namespace:GUIForDiskpart.userControls" x:Class="GUIForDiskpart.windows.Test"
        mc:Ignorable="d"
        Title="Test" Height="450" Width="800">
    <Grid Background="#FF3E3E3E">

        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <userControls:ConsoleReturnUI x:Name="ConsoleReturn" Grid.Row="1" Width="NaN" Height="NaN" Margin="10,10,10,10"/>
        <DataGrid d:ItemsSource="{d:SampleData ItemCount=5}"/>

    </Grid>
